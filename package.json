{
  "name": "room-assistant2",
  "version": "1.2.0",
  "description": "Presence tracking and more for automation on the room-level",
  "author": "Levi Govaerts <legovaer@me.com>",
  "license": "MIT",
  "repository": "github:legovaer/room-assistant2",
  "keywords": [
    "smart home",
    "home automation",
    "home assistant",
    "presence",
    "bluetooth",
    "thermopile"
  ],
  "homepage": "https://www.room-assistant.io",
  "bugs": "https://github.com/legovaer/room-assistant2/issues",
  "funding": "https://github.com/legovaer/room-assistant2?sponsor=1",
  "bin": "./bin/room-assistant.js",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "files": [
    "dist/**/*.{js,ts}",
    "dist/config/definitions/*.yml",
    "bin/*.js",
    ".github/FUNDING.yml"
  ],
  "scripts": {
    "prepack": "npm run build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prepare": "husky install",
    "postinstall": "github-sponsors",
    "format": "prettier --write \"src/**/*.ts\" \"bin/*.js\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test,bin}/**/*.{ts,js}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/core": "^8.0.4",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/platform-ws": "^8.0.4",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/websockets": "^8.0.4",
    "@willsoto/nestjs-prometheus": "^4.0.0",
    "2d-array-rotation": "^2.1.1",
    "async-mqtt": "^2.6.1",
    "async-mutex": "^0.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.1.1",
    "config": "^3.3.6",
    "democracy": "^3.1.3",
    "github-sponsors": "^1.0.1",
    "joiful": "^3.0.0",
    "js-yaml": "^4.0.0",
    "kalmanjs": "^1.1.0",
    "lodash": "^4.17.21",
    "mathjs": "^11.2.1",
    "nest-emitter": "^1.1.1",
    "nest-winston": "^1.6.0",
    "node-gyp": "^9.2.0",
    "please-upgrade-node": "^3.2.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "slugify": "^1.5.0",
    "swagger-ui-express": "^4.1.6",
    "systeminformation": "^5.6.12",
    "tar": "^6.1.11",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^19.1.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^8.0.4",
    "@types/config": "^3.3.0",
    "@types/express": "^4.17.11",
    "@types/i2c-bus": "^5.1.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.168",
    "@types/mdns": "0.0.34",
    "@types/node": "^14.14.37",
    "@types/supertest": "^2.0.11",
    "@types/ws": "^8.2.1",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.27",
    "@vuepress/plugin-pwa": "^2.0.0-beta.27",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "esm": "^3.2.25",
    "husky": "^8.0.1",
    "jest": "^27.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.6.2",
    "vuepress": "^2.0.0-beta.27",
    "webpack": "^5.53.0"
  },
  "optionalDependencies": {
    "@mkerix/noble": "1.9.2-11.1",
    "bleno": "npm:@abandonware/bleno@^0.5.1-4",
    "canvas": "^2.9.1",
    "i2c-bus": "^5.1.0",
    "mdns": "^2.5.1",
    "onoff": "^6.0.0",
    "sd-notify": "^2.8.0"
  },
  "peerDependencies": {
    "winston-elasticsearch": "^0.16.1",
    "winston-loki": "^6.0.5"
  },
  "peerDependenciesMeta": {
    "winston-elasticsearch": {
      "optional": true
    },
    "winston-loki": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12 < 17"
  },
  "lint-staged": {
    "*.{ts,js}": "eslint"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "testEnvironment": "node",
    "setupFiles": [
      "./env.ts"
    ]
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
